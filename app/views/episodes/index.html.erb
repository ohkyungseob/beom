<p id="notice"><%= notice %></p>

<h1>Episodes</h1>

<table>
  <thead>
    <tr>
      <th>Round</th>
      <th>Cookiecount</th>
      <th>Registration</th>
      <th>Content</th>
      <th>Scope</th>
      <th>Webtoon</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @episodes.each do |episode| %>
      <tr>
        <td><%= episode.round %></td>
        <td><%= episode.cookiecount %></td>
        <td><%= episode.registration %></td>
        <td><%= episode.content %></td>
        <td><%= episode.scope %></td>
        <td><%= episode.webtoon %></td>
        <td><%= link_to 'Show', episode %></td>
        <td><%= link_to 'Edit', edit_episode_path(episode) %></td>
        <td><%= link_to 'Destroy', episode, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Episode', new_episode_path %>
